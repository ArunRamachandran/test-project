{
  "name": "weather-application",
  "version": "1.0.0",
  "description": "A web app to display the weather details of pre-loaded cities",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "NODE_ENV=development webpack-dev-server --open --config webpack.dev.js",
    "build": "npm run prod",
    "prod": "rm -rf prod && mkdir prod && NODE_ENV=production webpack --config webpack.prod.js",
    "test": "./node_modules/.bin/jest --verbose --config jest.config.json",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "5.0.0",
    "redux": "4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "autoprefixer-loader": "^3.2.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "cors": "^2.8.5",
    "css-loader": "^3.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.18.3",
    "extract-loader": "^5.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^5.1.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^1.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4"
  },
  "author": "Arun K Ramachandran",
  "license": "ISC"
}
